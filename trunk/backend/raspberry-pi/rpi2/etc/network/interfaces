auto lo

iface lo inet loopback
iface eth0 inet dhcp

allow-hotplug wlan0
iface wlan0 inet static
address 192.168.88.1
gateway 192.168.88.1
netmask 255.255.255.0
network 192.168.88.0
broadcast 192.168.88.255

